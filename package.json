    "next": "15.1.6",
    "react": "19.0.0",
    "react-day-picker": "^9.5.0",
    "react-dom": "19.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@radix-ui/themes": "^3.2.0",
    "@storybook/addon-a11y": "^8.5.1",
