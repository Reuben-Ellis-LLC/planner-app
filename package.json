    "@types/react-color": "^3.0.13",
    "@types/react-dom": "19.0.3",
    "@vitest/browser": "^3.0.4",
    "@vitest/coverage-v8": "^3.0.4",
    "date-fns": "^4.1.0",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^9.19.0",
    "eslint-config-next": "15.1.6",
    "eslint-plugin-storybook": "^0.11.2",
    "jspdf": "^2.5.2",
    "playwright": "^1.50.0",
